{
  "name": "@letta-ai/vercel-ai-sdk-provider",
  "version": "0.0.3",
  "license": "MIT",
  "sideEffects": false,
  "main": "example/letta-ai-sdk-example/node_modules/@letta-ai/vercel-ai-sdk-provider/dist/index.js",
  "module": "example/letta-ai-sdk-example/node_modules/@letta-ai/vercel-ai-sdk-provider/dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "example/letta-ai-sdk-example/node_modules/@letta-ai/vercel-ai-sdk-provider/dist/**/*",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "clean": "rm -rf example/letta-ai-sdk-example/node_modules/@letta-ai/vercel-ai-sdk-provider/dist",
    "lint": "eslint \"./**/*.ts*\"",
    "type-check": "tsc --noEmit",
    "prettier-check": "prettier --check \"./**/*.ts*\"",
    "test": "npm test:node && npm test:edge",
    "test:edge": "vitest --config vitest.edge.config.js --run",
    "test:node": "vitest --config vitest.node.config.js --run",
    "test:node:watch": "vitest --config vitest.node.config.js"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "dependencies": {
    "@ai-sdk/provider": "1.1.0",
    "@ai-sdk/provider-utils": "2.2.4",
    "@ai-sdk/ui-utils": "^1.2.5",
    "@letta-ai/letta-client": "^0.1.80"
  },
  "devDependencies": {
    "@types/node": "20.17.24",
    "tsup": "^8",
    "typescript": "5.6.3",
    "vitest": "^3.1.1",
    "zod": "3.23.8"
  },
  "peerDependencies": {
    "zod": "^3.0.0"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://sdk.vercel.ai/docs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vercel/ai.git"
  },
  "bugs": {
    "url": "https://github.com/vercel/ai/issues"
  },
  "keywords": [
    "ai"
  ]
}
